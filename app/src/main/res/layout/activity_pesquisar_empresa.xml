<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".pesquisarEmpresa">

    <TextView
        android:id="@+id/title_pesquisarEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pesquisar Empresa"
        android:textSize="40sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_margin="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/ContainerComponentsPesquisa"
        style="@style/ContainerComponents"
        app:layout_constraintTop_toBottomOf="@+id/title_pesquisarEmpresa"
        app:layout_constraintStart_toStartOf="/parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <EditText
        android:id="@+id/digitarNomeEmpresa"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:hint="Nome da Empresa"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="@+id/ContainerComponentsPesquisa"
        app:layout_constraintStart_toStartOf="@+id/ContainerComponentsPesquisa"
        app:layout_constraintEnd_toEndOf="@+id/ContainerComponentsPesquisa"
        />

    <Button
        android:id="@+id/buttomPesquisarEmpresa"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:hint="Pesquisar"
        android:textColorHint="@color/white"
        android:textSize="20dp"


        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="264dp"
        tools:ignore="MissingConstraints" />



    <TextView
        android:id="@+id/resultadoEndereÃ§o"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="Recebimento"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="352dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="CEP"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="456dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>